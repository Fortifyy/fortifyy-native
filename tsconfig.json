{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "ES6",
    "target": "es5",
    "strict": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "jsx": "react-native"
  },
  "exclude": [
    "node_modules"
  ]
}
